[run]
source = .
omit = 
    */venv/*
    */env/*
    */migrations/*
    */settings/*
    */tests/*
    */test_*
    manage.py
    */wsgi.py
    */asgi.py
    */celery.py
    */conftest.py
    setup.py
    */node_modules/*
    */static/*
    */media/*
    */__pycache__/*
    */htmlcov/*
    */coverage/*
    */reports/*
    .coverage*
    coverage.json
    test_*.py
    *_test.py
    create_*.py
    run_tests.py
    test_system.py
    setup_system.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov
title = Banking System Coverage Report

[json]
output = coverage.json